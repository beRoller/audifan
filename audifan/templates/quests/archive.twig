{% extends "base.twig" %}

{% block title %}Audifan.net - Quests - Archive{% endblock %}

{% block pagetitle %}<a href="/quests/">Quests</a>: Archive{% endblock %}

{% block head %}
    <style>
            .archivetable {
                border: 2px solid #FFF;
                border-collapse: collapse;
                width: 300px;
                margin: auto;
            }
            
            .archivetable tr th {
                border: 1px solid #FFF;
                font-size: 10pt;
                font-weight: bold;
                padding: 3px;
                text-align: center;
            }
            
            .archivetable tr td {
                border: 1px solid #FFF;
                font-size: 9pt;
                padding: 3px;
                text-align: center;
            }
        </style>
{% endblock %}

