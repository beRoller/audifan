{% extends "base.twig" %}

{% block title %}Audifan.net - Patch Archive - {{ month }} {{ num[6:] }}, {{ num[:4] }}{% endblock %}

{% block pagetitle %}{{ month }} {{ num[6:] }}, {{ num[:4] }} Patch{% endblock %}

{% block body %}
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1 col-lg-8 col-lg-offset-2">
        <div class="text-center">
            <h3>
                <a href="/patch/">&lt; Back to Patch Archive</a>
            </h3>
        </div>
        <div>
            {% include "/patch/patchinfo/" ~ num ~ ".twig" %}
        </div>
    </div>
</div>
{% endblock %}